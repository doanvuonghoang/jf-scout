<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         name=""
         point="org.eclipse.ui.perspectives">
      <perspective
            name="scoutCore"
            icon="resources/icons/eclipse_scout.gif"
            fixed="false"
            class="com.jf.scout.ui.swt.core.perspective.Perspective"
            id="com.jf.scout.ui.swt.core.perspective.Perspective">
      </perspective>
   </extension>
   <extension
         name=""
         point="org.eclipse.ui.views">
      <view
            name="Center View"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.CenterView"
            id="com.jf.scout.ui.swt.core.views.CenterView">
      </view>
      <view
            name="West view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.WestView"
            id="com.jf.scout.ui.swt.core.views.WestView">
      </view>
      <view
            name="North view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.NorthView"
            id="com.jf.scout.ui.swt.core.views.NorthView">
      </view>
      <view
            name="North west view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.NorthWestView"
            id="com.jf.scout.ui.swt.core.views.NorthWestView">
      </view>
      <view
            name="North east view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.NorthEastView"
            id="com.jf.scout.ui.swt.core.views.NorthEastView">
      </view>
      <view
            name="East view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.EastView"
            id="com.jf.scout.ui.swt.core.views.EastView">
      </view>
      <view
            name="South view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.SouthView"
            id="com.jf.scout.ui.swt.core.views.SouthView">
      </view>
      <view
            name="South west view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.SouthWestView"
            id="com.jf.scout.ui.swt.core.views.SouthWestView">
      </view>
      <view
            name="South east view"
            allowMultiple="true"
            icon="resources/icons/eclipse_scout.gif"
            category="com.jf.scout.ui.swt.core.viewCategory"
            restorable="true"
            class="com.jf.scout.ui.swt.core.views.SouthEastView"
            id="com.jf.scout.ui.swt.core.views.SouthEastView">
      </view>
      <view
            name="Progress View"
            allowMultiple="false"
            icon="resources/icons/progress_view.gif"
            category="org.eclipse.ui"
            restorable="true"
            class="org.eclipse.ui.ExtensionFactory:progressView"
            id="org.eclipse.ui.views.ProgressView">
      </view>
      <category
            name="scoutCore"
            id="com.jf.scout.ui.swt.core.viewCategory">
      </category>
   </extension>
   <extension
         name=""
         point="org.eclipse.ui.startup">
      <startup
            class="com.jf.scout.ui.swt.core.SwtStartup">
      </startup>
   </extension>
   <extension
         id="application"
         name="Application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.jf.scout.ui.swt.core.application.Application">
         </run>
      </application>
   </extension>
   <extension
         name=""
         point="org.eclipse.ui.editors">
      <editor
            name="ScoutEditorPart"
            default="false"
            icon="resources/icons/eclipse_scout.gif"
            matchingStrategy="org.eclipse.scout.rt.ui.swt.window.desktop.editor.ScoutEditorMatchingStrategy"
            class="com.jf.scout.ui.swt.core.editor.ScoutEditorPart"
            id="com.jf.scout.ui.swt.core.editor.ScoutEditorPart">
      </editor>
   </extension>
   <extension
         id="product"
         name=""
         point="org.eclipse.core.runtime.products">
      <product
            name="scoutCore"
            application="com.jf.scout.ui.swt.core.application">
         <property
               name="appName"
               value="scoutCore">
         </property>
         <property
               name="applicationXMI"
               value="org.eclipse.platform/LegacyIDE.e4xmi">
         </property>
         <property
               name="cssTheme"
               value="org.eclipse.e4.ui.css.theme.e4_default">
         </property>
         <property
               name="applicationCSSResources"
               value="platform:/plugin/org.eclipse.platform/images/">
         </property>
      </product>
   </extension>
   <extension
         name=""
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <dynamic
               class="com.jf.scout.ui.swt.core.application.menu.DesktopMenuBar"
               id="org.eclipse.scout.swt.menu.dynamic">
         </dynamic>
      </menuContribution>
   </extension>

</plugin>
