<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         name=""
         point="org.eclipse.scout.service.services">
      <proxy
            factory="org.eclipse.scout.rt.client.services.ClientProxyServiceFactory"
            class="jfscout.shared.administration.services.IExtensionService">
      </proxy>
      <proxy
            factory="org.eclipse.scout.rt.client.services.ClientProxyServiceFactory"
            class="jfscout.shared.administration.services.IRoleService">
      </proxy>
      <proxy
            factory="org.eclipse.scout.rt.client.services.ClientProxyServiceFactory"
            class="jfscout.shared.administration.ui.forms.IUserService">
      </proxy>
      <proxy
            factory="org.eclipse.scout.rt.client.services.ClientProxyServiceFactory"
            class="jfscout.shared.administration.services.lookup.IRoleLookupService">
      </proxy>
      <proxy
            factory="org.eclipse.scout.rt.client.services.ClientProxyServiceFactory"
            class="jfscout.shared.administration.ui.forms.IAssignToRoleService">
      </proxy>
   </extension>
   <extension
         name=""
         point="org.eclipse.scout.rt.extension.client.desktopExtensions">
      <desktopExtension
            class="jfscout.client.administration.ui.desktop.DesktopExtension"
            active="true">
      </desktopExtension>
   </extension>

